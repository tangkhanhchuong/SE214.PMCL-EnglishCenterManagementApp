  "main": "./core/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node core/server.js",
    "dev": "nodemon core/server.js",
    "migrate": "knex migrate:latest --knexfile core/Database/knexfile.js",
    "undo-migrate": "knex migrate:rollback --knexfile core/Database/knexfile.js",
    "re-migrate": "npm run undo-migrate && npm run migrate",
    "make-migrate": "knex --knexfile core/Database/knexfile.js migrate:make",
    "seed": "knex seed:run --knexfile core/Database/knexfile.js",
    "undo-seed": "knex seed:rollback --knexfile core/Database/knexfile.js",
    "make-seed": "npx knex --knexfile core/Database/knexfile.js seed:make",
    "init-db": "npm run undo-migrate && npm run migrate && npm run seed"
  },
